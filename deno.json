{
  "tasks": {
    "tauri": "tauri",
    "build": "deno run -A ./src/build.ts",
    "build-watch": "deno run -A --watch=./src/src ./src/build.ts"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2"
  },
  "imports": {
    "preact": "npm:preact",
    "preact/hooks": "npm:preact/hooks",
    "preact/jsx-runtime": "npm:preact/jsx-runtime"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ]
  }
}
